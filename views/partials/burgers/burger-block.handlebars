<li class="col s12 m6 l4">
	<h6 style="font-weight:bolder">{{dataValues.name}}</h6><br/><br/>
	{{#if eaten}}
		<button class="col s12 green darken-2 change-eaten btn z-depth-4" style="height:60px;" data-id="{{dataValues.id}}" data-newEaten="{{eaten}}">
			EAT!
			<i class="large material-icons">fastfood</i>
		</button>
	{{else}}
		<button class="col s12 green darken-2 eat-again btn z-depth-4" style="height:60px;" data-id="{{dataValues.id}}" data-newEaten="{{eaten}}">
			SECONDS!
			<i class="large material-icons">fastfood</i>
		</button>
	{{/if}}
	<button class="col s12 red darken-4 delete-burger btn z-depth-4" style="height:40px;" data-id="{{dataValues.id}}">
		<i class="large material-icons">delete_forever</i>
	</button>
	<br/><br/><br/>
</li>
